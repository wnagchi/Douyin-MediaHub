{
  "name": "douyin-media-gallery",
  "version": "0.1.0",
  "private": true,
  "main": "app.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "node app.js --port 3001",
    "dev:client": "vite",
    "build": "vite build",
    "start": "node app.js",
    "scan": "node app.js --scan",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "antd": "^6.1.3",
    "ffmpeg-static": "^5.2.0",
    "masonic": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.30.2",
    "sharp": "^0.33.0",
    "swiper": "^11.1.5",
    "video-react": "^0.16.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/ui": "^4.0.18",
    "autoprefixer": "^10.4.23",
    "concurrently": "^8.2.2",
    "cross-env": "^10.1.0",
    "fake-indexeddb": "^6.2.5",
    "happy-dom": "^20.3.9",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^0.21.0",
    "vitest": "^4.0.18"
  }
}
